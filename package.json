{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "preload-fonts": "gatsby-preload-fonts"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.24.55",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.17",
    "gatsby-plugin-chakra-ui": "^0.1.4",
    "gatsby-plugin-manifest": "^2.4.28",
    "gatsby-plugin-preload-fonts": "^1.2.24",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-sharp": "^2.6.33",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.3.28",
    "gatsby-transformer-sharp": "^2.5.14",
    "node-sass": "^4.13.1",
    "nord": "^0.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.28.2",
    "react-marquee-slider": "^1.1.2",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
